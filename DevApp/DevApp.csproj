    </None>
    <None Update="Files\ManyCellTypes.xlsx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Files\OneCellNumberFillBorder - Copier.xlsx">
