    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Excelam\Excelam.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Files\ExcelEmpty.xlsx">
